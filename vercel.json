{
  "version": 2,
  "outputDirectory": "dist/public",
  "installCommand": "npm install --include=dev",
  "buildCommand": "npm run build",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "includeFiles": "server/**",
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/objects/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
